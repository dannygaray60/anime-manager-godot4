; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="AnimeManager"
run/main_scene="res://main_menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="AnimeManager"
config/quit_on_go_back=false
config/features=PackedStringArray("4.3", "Mobile")
run/low_processor_mode=true
boot_splash/bg_color=Color(0.4, 0.309804, 0.639216, 1)
boot_splash/image="res://assets/victor_mare.png"
config/icon="res://assets/icon2.png"
addons/icon_finder/preview_size=24

[autoload]

Config="*res://src/scripts/config.gd"

[display]

window/size/viewport_width=720
window/size/viewport_height=1280
window/size/window_width_override=480
window/size/window_height_override=853
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"
window/handheld/orientation=1

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
